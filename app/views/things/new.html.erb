<div class="col-md-6 col-md-offset-3">
    <div class="jumbotron frontpage">
      <h4>Select Date to View Nasa Image of the Day</h4>

      <%= form_for(@thing) do |f|  %>

    <%= f.label :date %>
    <%= f.date_select :date, use_two_digit_numbers: true, start_year: 1990, end_year: 2015, class: 'form-control' %>


    <%= f.submit "Choose Date", class: 'btn btn-primary' %>

<% end %>
  </div>
</div>
